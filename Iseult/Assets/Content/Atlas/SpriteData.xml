<?xml version="1.0" encoding="utf-8" ?>
<sprites>

  <sprite_string id="iseult">
    <Texture>characters/iseult</Texture>
    <FrameWidth>128</FrameWidth>
    <FrameHeight>128</FrameHeight>
    <OriginX>64</OriginX>
    <OriginY>64</OriginY>
    <Animations>
      <Anim id="idle" delay=".1" frames="0"/>
      <Anim id="walk" delay=".1" frames="0"/>
	    <Anim id="crouchIn" delay=".1" loop="False" frames="0"/>
	    <Anim id="crouchOut" delay=".1" loop="False" frames="0"/>
  	  <Anim id="jumpUp" delay=".1" loop="False" frames="0"/>
	    <Anim id="jumpDown" delay=".1" loop="True" frames="0"/>
	    <Anim id="idleHolding" delay=".1" frames="0"/>
	    <Anim id="walkHolding" delay=".1" frames="0"/>
	    <Anim id="jumpUpHolding" delay=".1" loop="False" frames="0"/>
	    <Anim id="jumpDownHolding" delay=".1" loop="True" frames="0"/>
	    <Anim id="idleThrowing" delay=".1" loop="False" frames="0"/>
	    <Anim id="jumpThrowing" delay=".1" loop="False" frames="0"/>
    </Animations>
  </sprite_string>
  
  <!--Environmental-->

  <sprite_string id="door">
    <Texture>environment/doorway</Texture>
    <FrameWidth>96</FrameWidth>
    <FrameHeight>128</FrameHeight>
    <OriginX>0</OriginX>
    <OriginY>0</OriginY>
    <Animations>
      <Anim id="stand" delay=".1" frames="0"/>
      <Anim id="open" delay=".1" loop="false" frames="0"/>
    </Animations>
  </sprite_string>
  
</sprites>
